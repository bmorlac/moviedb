{
  "name": "moviedb",
  "version": "1.0.0",
  "description": "A database to keep track of all kinds of movies, series you own.",
  "main": "dist/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require test/chai.config --timeout 20000 test/**/*.test.js",
    "start": "nodemon --exec babel-node src/index.js --inspect",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel src --out-dir dist --copy-files",
    "serve": "node dist/index.js"
  },
  "author": "bmorlac",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.1",
    "express": "^4.15.4",
    "graphql": "^0.11.3",
    "graphql-server-express": "^1.1.2",
    "graphql-tools": "^1.2.2",
    "lodash": "^4.17.4",
    "pg": "^7.3.0",
    "sequelize": "^4.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.5.3",
    "nodemon": "^1.12.0",
    "sequelize-cli": "^3.0.0-2",
    "umzug": "^2.0.1"
  }
}
